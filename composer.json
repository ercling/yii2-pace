{
    "name": "vensonk/yii2-pace",
    "type": "yii2-extension",
    "description": "Automatic page load progress bar. The PACE plugin integration for Yii2 Framework",
    "keywords": ["yii2", "PACE", "loading"],
    "license": "BSD-3-Clause",
    "homepage": "https://github.com/vensonk/yii2-pace",
    "support": {
        "issues": "https://github.com/vensonk/yii2-pace/issues",
        "source": "https://github.com/vensonk/yii2-pace"
    },
    "authors": [
        {
            "name": "Life Net Inc.",
            "email": "venson@thelifenet.com",
            "homepage": "http://www.thelifenet.com",
            "role": "Developer"
        }
    ],
    "require": {
        "yiisoft/yii2": "~2.0.0"
    },
    "autoload": {
        "psr-4": {
            "vensonk\\pace\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
